{
  "name": "jquery-plugin-ts-starter",
  "version": "0.2.0",
  "description": "A simple starter project for developing jQuery plugins with Typescript and Webpack.",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist && rimraf lint-reports",
    "build:dev": "npm run clean:dist && webpack --mode=development --progress --profile --color",
    "build:prod": "npm run clean:dist && npm run test && webpack --mode=production --progress --profile --color",
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "test": "karma start --no-auto-watch --single-run",
    "test:watch": "karma start --auto-watch --no-single-run"
  },
  "keywords": [
    "jquery",
    "plugin",
    "starter",
    "typescript",
    "webpack"
  ],
  "author": "Stephan Zerhusen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/szerhusenBC/jquery-plugin-ts-starter.git"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/jquery": "~3.5.6",
    "@types/node": "~16.4.10",
    "copy-webpack-plugin": "~9.0.1",
    "html-webpack-plugin": "^5.3.2",
    "jasmine-core": "^3.8.0",
    "jquery": "3.6.0",
    "karma": "^6.3.4",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "~3.0.2",
    "source-map-loader": "~3.0.0",
    "ts-loader": "~9.2.4",
    "ts-node": "~10.1.0",
    "tslint": "~6.1.3",
    "tslint-loader": "~3.5.4",
    "typescript": "~4.3.5",
    "webpack": "~5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
